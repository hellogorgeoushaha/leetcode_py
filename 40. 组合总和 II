class Solution(object):
    def combinationSum2(self, candidates, target):
        """
        :type candidates: List[int]
        :type target: int
        :rtype: List[List[int]]
        """
        candidates.sort()
        self.ans = []
        self.dfs(candidates, [], target, 0)
        return self.ans
        
    def dfs(self, candidates, res, target, start):
        if target == 0:
            self.ans.append(res)
            
        for i in range(start, len(candidates)):
            if i>start and candidates[i]==candidates[i-1]: # important
                continue

            if candidates[i] > target:
                return 
            else:
                self.dfs(candidates, res+[candidates[i]], target - candidates[i], i+1)
              
